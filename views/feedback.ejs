<!DOCTYPE html>
<html lang="en">
<head>
    <% include partials/template/head.ejs %> 
</head>
<body>
    <% include partials/template/header.ejs %> 
    <div class="container-fluid">
        <form class="feedback-form">
            <h2>Please Give a Feedback!</h2>
            <input type="text"  id="feedbackForm-name" placeholder="Enter your name">
            <input type="text"  id="feedbackForm-title" placeholder="Title of your feedback">
            <textarea type="text" placeholder="Enter your message, then hit the submit"  id="feedbackForm-message" rows="2"></textarea>
            <button type="submit" class="btn btn-default">Submit</button>
        </form>
    </div>

    <aside class="col-sm-8">
        <div class="feedback" id="feedbackMessage">
            <h2 class="article-title">Recent Feedback</h2>
            <div class="feedback-messages"></div>
        </div>
    </aside>
    <% include partials/template/scripts.ejs %>
</body>
</html>